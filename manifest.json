{
    "manifest_version": 3,
    "name": "NTS Plus",
    "version": "0.1",
    "description": "Enhancements for NTS radio.",
    "content_scripts": [
      {
        "matches": ["https://www.nts.live/*"],
        "js": ["nts.js"]
      },
      {
        "matches": ["https://player-widget.mixcloud.com/*"],
        "js": ["mixcloud.js"],
        "all_frames": true
      }
    ]
  }